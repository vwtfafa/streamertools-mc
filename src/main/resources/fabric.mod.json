{
  "schemaVersion": 1,
  "id": "streamertools",
  "version": "${version}",
  "name": "Streamer Tools",
  "description": "",
  "authors": [],
  "contact": {},
  "license": "MIT",
  "icon": "assets/streamertools/icon.png",
  "environment": "*",
  "entrypoints": {
    "fabric-datagen": [
      "org.vwtfafa.streamertools.client.StreamerToolsModDataGenerator"
    ],
    "client": [
      "org.vwtfafa.streamertools.client.StreamerToolsModClient"
    ],
    "main": [
      "org.vwtfafa.streamertools.StreamerToolsMod"
    ]
  },
  "mixins": [
